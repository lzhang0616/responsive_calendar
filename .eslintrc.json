{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "comma-dangle": 0,
    "import/no-unresolved": 1,
    "prefer-const": [1, {
      "destructuring": "any",
      "ignoreReadBeforeAssign": true
    }]
  }
}
