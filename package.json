  "repository": {
    "type": "git",
    "url": "git@github.com:lzhang0616/responsive_calendar.git"
  },
  "keywords": [
    "calendar",
    "responsive",
    "react",
    "redux"
  ],
  "author": "Li Zhang, Gary Thomas",
  "license": "MIT",
  "bugs": {
    "url": "github.com/lzhang0616/responsive_calendar/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1",
      "stage-2"
    ]
  },
  "homepage": "github.com/lzhang0616/responsive_calendar",
  "dependencies": {
    "babel-polyfill": "~6.9.1",
    "isomorphic-fetch": "~2.2.1",
    "lodash": "~4.13.1",
    "moment": "~2.13.0",
    "react": "~0.14.8",
    "react-dom": "~0.14.8",
    "react-redux": "~4.4.5",
    "redux": "~3.5.2",
    "redux-thunk": "~2.1.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "autoprefixer": "~6.3.6",
    "babel-core": "~6.9.0",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-object-assign": "~6.8.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-es2015-webpack": "~6.4.1",
    "babel-preset-react": "~6.5.0",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-1": "~6.5.0",
    "babel-preset-stage-2": "~6.5.0",
    "babel-register": "~6.9.0",
    "css-element-queries": "~0.3.2",
    "css-loader": "~0.23.1",
    "eslint": "~2.13.1",
    "eslint-config-airbnb": "~9.0.1",
    "eslint-plugin-import": "~1.8.1",
    "eslint-plugin-jsx-a11y": "~1.5.3",
    "eslint-plugin-react": "~5.2.2",
    "express": "~4.13.4",
    "mocha": "~2.5.3",
    "morgan": "~1.7.0",
    "nightmare": "^2.5.2",
    "node-sass": "~3.8.0",
    "nyc": "~6.6.1",
    "postcss": "~5.0.21",
    "postcss-loader": "~0.9.1",
    "sass-loader": "~3.2.1",
    "style-loader": "~0.13.1",
    "webpack": "~2.1.0-beta.13",
    "webpack-dev-middleware": "~1.5.1",
    "webpack-hot-middleware": "~2.10.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "dev_server.js",
      "webpack.dev.config.js",
      "webpack.config.js"
    ]
